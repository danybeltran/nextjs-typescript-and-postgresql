datasource db {
  provider = "postgresql"
}

generator client {
  provider = "prisma-client-js"
}

model Post {
  id      Int      @id @default(autoincrement())
  title   String
  date    DateTime @default(now())
  content String
}

model Preferences {
  id                   Int    @id @default(autoincrement())
  user_email           String
  user_fullname        String
  user_profile_picture String
  username             String
  user_description     String
  // Add more customizations
}
